---
import { storyblokEditable } from '@storyblok/astro'
import StoryblokComponent from '@storyblok/astro/StoryblokComponent.astro';

const { blok } = Astro.props;
---

<nav class="bg-emerald-600 dark:bg-gray-900 fixed w-full z-20 top-0 start-0 border-b border-gray-200 dark:border-gray-600">
  <div class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4">
    <!-- Logo and Brand Name -->
    <a href="/" class="flex items-center space-x-3 rtl:space-x-reverse">

      <span class="self-center text-2xl font-semibold whitespace-nowrap dark:text-white">{blok.brandname}</span>
    </a>

    

    <!-- Navbar Links -->
    <div class="items-center justify-between hidden w-full md:flex md:w-auto md:order-1" id="navbar-sticky">

        {blok.columns?.map((columnBlok) => (
          
              <StoryblokComponent blok={columnBlok} />
            
          
        ))}
      </ul>
    </div>
  </div>
</nav>
